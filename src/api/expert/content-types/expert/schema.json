{
  "kind": "collectionType",
  "collectionName": "experts",
  "info": {
    "singularName": "expert",
    "pluralName": "experts",
    "displayName": "Expert",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "jobTitle": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "expertise": {
      "type": "component",
      "repeatable": true,
      "component": "array-strings.strings",
      "max": 5,
      "required": true
    },
    "credentials": {
      "type": "component",
      "repeatable": true,
      "component": "array-strings.strings",
      "max": 7,
      "required": true
    },
    "topics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "mappedBy": "experts"
    },
    "specialtyTitle": {
      "type": "string",
      "required": false
    },
    "languages": {
      "type": "component",
      "repeatable": true,
      "component": "array-strings.strings",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Gender-Neutral",
        "Non-conforming",
        "Non Binary",
        "Agender",
        "Bigender",
        "Trans man",
        "Trans woman",
        "Rather not say"
      ],
      "required": true
    },
    "modules": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::module.module",
      "inversedBy": "experts"
    },
    "bio": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "experts"
    },
    "Pronouns": {
      "type": "enumeration",
      "enum": [
        "He/Him",
        "She/Her",
        "They/They",
        "Rather not say"
      ],
      "required": true
    },
    "videoIntro": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "bannerImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "array-two-strings.faq",
      "max": 3
    },
    "reviews": {
      "type": "component",
      "repeatable": true,
      "component": "array-two-strings.review-array"
    },
    "iAlsoHaveExperienceIn": {
      "type": "component",
      "repeatable": true,
      "component": "array-strings.strings"
    },
    "programmes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::programme.programme",
      "mappedBy": "expert"
    },
    "isDemo": {
      "type": "boolean",
      "default": false
    },
    "overview": {
      "type": "text",
      "required": false,
      "maxLength": 120
    },
    "institutionsSupported": {
      "type": "component",
      "repeatable": true,
      "component": "array-two-strings.array-images",
      "max": 8
    },
    "sessionExpectations": {
      "type": "component",
      "repeatable": true,
      "component": "array-two-strings.session-expectations",
      "required": false,
      "max": 2
    },
    "askMeAbout": {
      "type": "component",
      "repeatable": true,
      "component": "array-strings.strings"
    },
    "article": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article.article",
      "mappedBy": "expert"
    },
    "title": {
      "type": "string"
    }
  }
}
